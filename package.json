    "fs-extra": "^7.0.1",
    "inquirer": "^2.0.0",
    "insight": "^0.10.1",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^5.1.0",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "safe-json-stringify": "^1.2.0",
    "shelljs": "^0.8.3",
    "to-absolute-glob": "^2.0.2",
